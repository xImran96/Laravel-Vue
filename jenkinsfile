pipeline {
    agent any
    // agent {'label of agent'}
    stages {
            stage('Composer installation'){
               steps {
                sh "echo initializing composer"
                sh "composer install";
                }
            }
            stage('Vue Installation'){
                steps {
                    sh "echo initializing composer"
                    sh "npm install"
                    sh "npm run dev"

                }
            }
    }
}